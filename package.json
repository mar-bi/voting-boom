{
  "name": "voting-boom",
  "version": "1.0.0",
  "description": "react, mongo, and node voting app",
  "engines": {
    "node": "6.12.3"
  },
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mar-bi/voting-boom"
  },
  "scripts": {
    "start": "node server.js",
    "create": "webpack",
    "dev": "webpack-dev-server --open",
    "build": "NODE_ENV='production' webpack -p",
    "format": "prettier --trailing-comma none --no-semi --single-quote --write \"app/**/*.js\"",
    "serve": "nodemon server.js"
  },
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-1"
    ]
  },
  "keywords": [
    "mongo",
    "mongoose",
    "react",
    "react-router",
    "express",
    "node",
    "d3",
    "material-ui",
    "passport",
    "jsonwebtoken"
  ],
  "author": "mar-bi",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "d3": "^4.12.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.1",
    "material-ui": "^0.19.4",
    "mongoose": "^4.13.9",
    "morgan": "^1.9.0",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-router-dom": "^4.2.2",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.17.0",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.1",
    "prettier": "1.9.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  }
}
